mixin b_block_5()
	tr
		td.td_block(style="padding:30px 20px; background:white; border-bottom:1px dashed #cfcfcf")
			table.table_normal(width="235" align="center" cellspacing="0" cellpadding="0")
				tr
					td(style="width:23px; height:42px")
						img(src='/img/btn_white_left.jpg' style="width:23px; height:42px; display:block; margin:0")
					td(valign="middle" style="text-align:center; border-top:1px solid #e6e1ec; border-bottom:1px solid #e6e1ec;")
						a(href="#" style="color:#5f5963; text-decoration:none; height:18px; padding:11px 0")
						| Все астрологи
					td(style="width:23px; height:42px")
						img(src='/img/btn_white_right.jpg' style="width:23px; height:42px; display:block; margin:0")
